#include <bits/stdc++.h>using namespace std; int main() {    ios::sync_with_stdio(false);    cin.tie(NULL);   int t;   cin>>t;   while (t--){    int n;    int k;    cin>>n>>k;    vector<long long>arr(n);       for(int i=0;i<n;i++) cin>>arr[i];    int count=0;    for(int i=0;i<n;i++){        if(arr[i]==k){            count++;            break;        }     }    if(count==0){        cout<<"NO \n";    }else cout<<"YES \n";    }     return 0;}